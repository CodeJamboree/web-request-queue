{
  "compilerOptions": {
    "target": "ES2016",
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node10",
    "types": [
      "node",
    ],
    "outDir": "build",
    "importHelpers": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}